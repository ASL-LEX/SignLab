<div class="container-fluid h-100">
  <div class="row h-100">
    {{>dashboardNav}}
    <div class="col-sm-9 col-md-10">
      <div style="padding: 30px">
        <h3>Backup</h3>
        <hr>
        <div class="row">
          <div class="col-md-2 col-xs-6">
            <button class="btn btn-primary" onclick="backup()">Create Backup</button>
          </div>
          <div class="col-md-2 col-xs-6">
            <button class="btn btn-secondary" onclick="clean()">Sync Backups</button>
          </div>
        </div>
        <br>
        <div class="card">
          <div style="padding: 20px;">
            <table id="table" class="table table-striped table-bordered" cellspacing="0" width="100%">
              <thead>
              <tr>
                <th>ID</th>
                <th>Backup ID</th>
                <th>Zip</th>
                <th>Time</th>
                <th>Restore</th>
                <th>Delete</th>
              </tr>
              </thead>
            </table>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>
<script type="text/javascript" src="/public/scripts/backups.js"></script>
